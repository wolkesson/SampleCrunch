<Window x:Class="Sample_Crunch.WebWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sample_Crunch"
        mc:Ignorable="d"
        Title="{Binding Title}" Height="480" Width="640" WindowStartupLocation="CenterOwner" WindowStyle="ToolWindow">
    <Grid>

        <WebBrowser x:Name="browser" local:WebBrowserUtility.BindableSource = "{Binding Address}" />
        
    </Grid>
</Window>
